{
    "name": "davidmintz/court-interpreters-office",
    "description": "web application for managing a busy US District Court interpreters office",
    "keywords": [
        "court interpreters", "federal court","US District Court","zf3","zend framework"
    ],
    "homepage": "https://davidmintz.org/",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^5.6 || ^7.0",
        "zendframework/zend-component-installer": "^1.0 || ^0.3 || ^1.0.0-dev@dev",
        "zendframework/zend-mvc": "^3.0.1",
        "zfcampus/zf-development-mode": "^3.0",
        "doctrine/doctrine-orm-module": "^1.1",
        "zendframework/zend-developer-tools": "^1.1",
        "zendframework/zend-test": "^3.0",
        "doctrine/data-fixtures": "^1.2",
        "zendframework/zend-code": "^3.1",
        "zendframework/zend-i18n": "^2.7",
        "zendframework/zend-mvc-plugin-flashmessenger": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "InterpretersOffice\\": "module/InterpretersOffice/src/",
            "InterpretersOffice\\Admin\\": "module/Admin/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ApplicationTest\\": "module/InterpretersOffice/test/"
        }
    },
    "extra": [],
    "scripts": {
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "development-disable": "zf-development-mode disable",
        "development-enable": "zf-development-mode enable",
        "development-status": "zf-development-mode status",
        "post-create-project-cmd": [
            "@development-enable"
        ],
        "serve": "php -S 0.0.0.0:8080 -t public/ public/index.php",
        "test": "phpunit"
    }
}
