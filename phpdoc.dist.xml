<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
  <parser>
    <target>docs/parser</target>
  </parser>
  <transformer>
    <target>docs/api</target>
  </transformer>
  <files>
    <directory>module</directory>
    <ignore>module/*test/*</ignore>
    <ignore>module/*config/*</ignore>
    <ignore>module/*/view/*</ignore>
    <ignore>module/Sandbox/*</ignore>
  </files>
</phpdoc>
