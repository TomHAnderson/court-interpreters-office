<?xml version="1.0" encoding="UTF-8"?>
<phpunit colors="true" bootstrap="module/InterpretersOffice/test/Bootstrap.php">
    <testsuites>
        <testsuite name="CourtInterpretersOffice Test Suite">
            <directory>./module/InterpretersOffice/test</directory>
            <directory>./module/Admin/test</directory>
            <exclude>./module/InterpretersOffice/test/AbstractControllerTest.php</exclude>
      
        </testsuite>
    
    </testsuites>
	<php>	 
	  <ini name="error_log" value="./data/test.error.log"/>	 
	  <ini name="log_errors" value="On"/>	 
	</php>
    <filter>
        <whitelist processUncoveredFilesFromWhitelist="true">
          <directory suffix=".php">./module/Admin/src</directory>
          <directory suffix=".php">./module/InterpretersOffice/src</directory>
          <!--
          <exclude>
              <directory>./module/InterpretersOffice/test/DataFixture</directory>
          </exclude>
          -->
        </whitelist>
    </filter>
</phpunit>
